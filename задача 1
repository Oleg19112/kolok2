#include <stdio.h>
int main()
{
    int z, positive = 0;
    do
    {
        printf("Ведіть кільскість: ");
        scanf("%i", &n); 
    }
    while ( z < 1 || z > 100);
    int N[z];
    for (int i = 0; i < z; i++)
    {
        scanf("%i", &N[i]);
        if ( N[i] > 0) positive++;
    }
    printf("%i\n", positive);
    if (positive!=0){
        for (int i = 0; i < n; i++)
        {
            if ( N[i] > 0) printf("%i ", N[i]);
        }
    }
    else printf("NO");
}
